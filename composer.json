{
    "name": "petk/phprst",
    "type": "library",
    "description": "reStructuredText PHP library",
    "keywords": ["rst"],
    "homepage": "https://github.com/petk/phprst",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Kokot",
            "email": "peterkokot@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4" : {
            "Rst\\" : "src/Rst/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
